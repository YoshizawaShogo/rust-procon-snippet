  "PRIME_FACTORIZATION": {
    "prefix": "PRIME_FACTORIZATION",
    "body": [
      "pub fn prime_factorization(mut num: usize) -> Vec<usize> {",
      "    let end = (num as f64).sqrt().floor() as usize;",
      "    let mut primes = Vec::new();",
      "    for i in 2..=end {",
      "        while num % i == 0 {",
      "            primes.push(i);",
      "            num /= i;",
      "        }",
      "    }",
      "    if primes.len() == 0 {",
      "        primes.push(num);",
      "    }",
      "    primes",
      "}"
    ]
